<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jnlp PUBLIC "-//Sun Microsystems, Inc.//DTD JNLP 1.5//EN"
                      "http://java.sun.com/dtd/JNLP-1.5.dtd">
<jnlp codebase="http://xpontus.sourceforge.net/snapshot/"
    href="$outputFile" spec="1.5+">
    <information>
        <title>XPontus XML Editor</title>
        <vendor>Yves Zoundi</vendor>
        <homepage href="http://xpontus.sourceforge.net/"/>
        <description kind="one-line">A free Java Based XML Editor</description>
        <icon href="xpontus.png"/>
        <shortcut online="false">
            <desktop/>
            <menu submenu="XPontus"/>
        </shortcut>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se href="http://java.sun.com/products/autodl/j2se"
            initial-heap-size="32m" max-heap-size="256m" version="1.5+"/>
        <property name="xpontusisjavawebstart" value="true"/> 
     $dependencies
  </resources>
    <application-desc main-class="$mainClass"/>
</jnlp>
